<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="250.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="ru.slukin.chat.client.controllers.RegController">
    <children>
        <Label layoutX="56.0" layoutY="51.0" text="Логин:"/>
        <TextField fx:id="loginField" layoutX="106.0" layoutY="47.0" onAction="#tryToRegister"/>
        <Label layoutX="39.0" layoutY="104.0" text="Никнейм:"/>
        <TextField fx:id="nicknameField" layoutX="106.0" layoutY="100.0" onAction="#tryToRegister"/>
        <Label layoutX="49.0" layoutY="152.0" text="Пароль:"/>
        <PasswordField fx:id="passwordField" layoutX="106.0" layoutY="148.0"/>
        <Button layoutX="108.0" layoutY="198.0" mnemonicParsing="false" onAction="#tryToRegister" text="Регистрация"/>
    </children>
</AnchorPane>
